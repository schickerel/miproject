<?xml version="1.0" encoding="UTF-8"?>
<config>
  <propel>
    <datasources default="migrationstreams">
      <datasource id="migrationstreams">
        <adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
        <connection>
          <dsn>mysql:host=localhost;dbname=migrationstreams</dsn>
          <user>root</user>
          <password></password>
          <settings>
            <setting id="charset">utf8</setting>
          </settings>
          <options>
            <option id="MYSQL_ATTR_INIT_COMMAND">SET NAMES utf8 COLLATE utf8_general_ci</option>
          </options>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>